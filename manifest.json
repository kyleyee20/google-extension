{
  "name": "To Do list",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Allows for quick and easy todo list",
  "homepage_url": "http://www.google.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "shortcuts",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "storage"
  ]
}